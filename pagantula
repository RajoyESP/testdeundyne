        wait(0.1)
        firetouchinterest(game.Players.LocalPlayer.Character["Left Leg"], game:GetService("Workspace").D7Doors.FreshDoor.Door, 1)
        wait(0.1)
        firetouchinterest(game.Players.LocalPlayer.Character["Left Leg"], game:GetService("Workspace").D7Doors.FreshDoor.Door, 0)
