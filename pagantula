--Morphs
function equipmorph()
        spawn(function()
        local args = {
            [1] = game:GetService("Players").LocalPlayer,
            [2] = "ReaperSansMorph" --Reaper Sans
        }
        game:GetService("ReplicatedStorage").MorphEquipRE:FireServer(unpack(args))
    end)
end

function equipmorph2()
    spawn(function()
    local args = {
        [1] = game:GetService("Players").LocalPlayer,
        [2] = "KillerSansMorph" --Killer Sans
    }
    game:GetService("ReplicatedStorage").MorphEquipRE:FireServer(unpack(args))
end)
end

