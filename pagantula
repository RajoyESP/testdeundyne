function collectpumps() --Auto collect pumpkins
    spawn(function()
        while getgenv().autograbpumpkin == true do
            for i, v in pairs(game:GetService("Workspace").Halloween:GetDescendants()) do
                if v.Name == "jackolantern" then
                firetouchinterest(game.Players.LocalPlayer.Character.Head, v, 1)
                wait(0.1)
                firetouchinterest(game.Players.LocalPlayer.Character.Head, v, 0)
                end
            end
        end
    end)
end

function buyfud() --Buy Mettaton Face's Steak
    spawn(function()
        fireclickdetector(workspace.FaceStake.ClickDetector)
    end)
end

function autobuyfud() --Auto buy Mettaton Face's Steak
    spawn(function()
        while getgenv().autofood == true do
            fireclickdetector(workspace.FaceStake.ClickDetector)
            wait(0.5)
        end
    end)
end

function buyfud2() --Buy Huss Puppies
    spawn(function()
        fireclickdetector(workspace.ExtraBossMaps.Stand.FaceStake.ClickDetector)
    end)
end

function autobuyfud2() --Auto buy Huss Puppies
    spawn(function()
        while getgenv().autofood2 == true do
            fireclickdetector(workspace.ExtraBossMaps.Stand.FaceStake.ClickDetector)
            wait(0.5)
        end
    end)
end

function killpoporo()
    spawn(function()
        while getgenv().autonightp == true do
            for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
                if v.Name == "Nightmare Papyrus" then
                    v.Humanoid.Health = 0
                end
            end
            wait(0.1)
        end
    end)
end

function killsuns()
    spawn(function()
        while getgenv().autonights == true do
            game:GetService("Workspace")["Nightmare Sans"].RealHealth.Value = 300
            wait(0.1)
        end 
    end)
end

function gotod7()
    spawn(function()
        fireclickdetector(game:GetService("Workspace").D7Wheel.ClickDetector)
    end)
end

