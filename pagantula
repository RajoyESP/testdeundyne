function tptoerror() --Error Sans
    spawn(function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(15799.9219, 25861.7676, 575.419556)
    end)
end

--Miscellanious
function autoFight() --Auto grab pumpkins -> fight Halloween Toriel / Papyrus
    while getgenv().autofighthal == true do
        for i, v in pairs(game:GetService("Workspace").Halloween:GetDescendants()) do
            if v.Name == "jackolantern" then
            firetouchinterest(game.Players.LocalPlayer.Character.Head, v, 1)
            wait(0.1)
            firetouchinterest(game.Players.LocalPlayer.Character.Head, v, 0)
            wait(1)
            firetouchinterest(game.Players.LocalPlayer.Character.Head, workspace.Halloween.HalloweenDoor1.Door, 1)
            wait(0.1)
            firetouchinterest(game.Players.LocalPlayer.Character.Head, workspace.Halloween.HalloweenDoor1.Door, 0)
            end
        end
    end
end

function collectpumps() --Auto collect pumpkins
    spawn(function()
        while getgenv().autograbpumpkin == true do
            for i, v in pairs(game:GetService("Workspace").Halloween:GetDescendants()) do
                if v.Name == "jackolantern" then
                firetouchinterest(game.Players.LocalPlayer.Character.Head, v, 1)
                wait(0.1)
                firetouchinterest(game.Players.LocalPlayer.Character.Head, v, 0)
                end
            end
        end
    end)
end

function buyfud() --Buy Mettaton Face's Steak
    spawn(function()
        fireclickdetector(workspace.FaceStake.ClickDetector)
    end)
end

function autobuyfud() --Auto buy Mettaton Face's Steak
    spawn(function()
        while getgenv().autofood == true do
            fireclickdetector(workspace.FaceStake.ClickDetector)
            wait(0.5)
        end
    end)
end

function infiniteyield() --Infinite Yield
    spawn(function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
    end)
end

function simpleremote() --Simple Remote Spy
    spawn(function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/exxtremestuffs/SimpleSpySource/master/SimpleSpy.lua"))()
    end)
end

function ut3dbypass() --UT3DBB Bypass (NOT WORKING!!!)
    spawn(function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/emkodelirdi/EmkoDelirdi/main/ut3dbbbypass"))()
    end)
end

