local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Undertale 3D Boss Battles", IntroEnabled = true, IntroText = "Paquito's Productions", HidePremium = false, SaveConfig = true, ConfigFolder = "Facherumtest"})

--Values
getgenv().autofighthal = true;
getgenv().autograbpumpkin = true;
getgenv().autofood = true;
getgenv().autofood2 = true;
getgenv().autonights = true;
getgenv().autonightp = true;
getgenv().autotouchu = true;
getgenv().autotouchufu = true;
getgenv().autotouchfellas = true;
getgenv().everyautotouchfellas = true;
getgenv().poronguilla = true;

