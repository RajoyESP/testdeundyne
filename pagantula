function toalphysness() --Alphys / Ness
    spawn(function()
        firetouchinterest(game.Players.LocalPlayer.Character["Left Leg"], game:GetService("Workspace").D7Doors.MysteryDoor.Door, 0)
        wait(0.1)
        firetouchinterest(game.Players.LocalPlayer.Character["Left Leg"], game:GetService("Workspace").D7Doors.MysteryDoor.Door, 1)
        wait(0.1)
        firetouchinterest(game.Players.LocalPlayer.Character["Left Leg"], game:GetService("Workspace").D7Doors.MysteryDoor.Door, 0)
        wait(0.1)
        firetouchinterest(game.Players.LocalPlayer.Character["Left Leg"], game:GetService("Workspace").D7Doors.MysteryDoor.Door, 1)
        wait(0.1)
        firetouchinterest(game.Players.LocalPlayer.Character["Left Leg"], game:GetService("Workspace").D7Doors.MysteryDoor.Door, 0)
        wait(0.1)
        firetouchinterest(game.Players.LocalPlayer.Character["Left Leg"], game:GetService("Workspace").D7Doors.MysteryDoor.Door, 1)
    end)
end

--Test
function touchundyne()
    spawn(function()
        while getgenv().autotouchu == true do
            firetouchinterest(game:GetService("Workspace")["Undyne"].HumanoidRootPart, game:GetService("Workspace")["imnot_thebest459"].Chest.MagicBall,  1)
            wait(0.1)
            firetouchinterest(game:GetService("Workspace")["Undyne"].HumanoidRootPart, game:GetService("Workspace")["imnot_thebest459"].Chest.MagicBall,  0)
        end
    end)
end

function touchundyneofundying()
    spawn(function()
        while getgenv().autotouchufu == true do
            firetouchinterest(game:GetService("Workspace")["Undyne the Undying"].HumanoidRootPart, game:GetService("Workspace")["imnot_thebest459"].Chest.MagicBall,  1)
            wait(0.1)
            firetouchinterest(game:GetService("Workspace")["Undyne the Undying"].HumanoidRootPart, game:GetService("Workspace")["imnot_thebest459"].Chest.MagicBall,  0)
        end
    end)
end

