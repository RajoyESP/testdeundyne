function tptoerror()
    spawn(function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(15799.9219, 25861.7676, 575.419556)
    end)
end

function infiniteyield()
    spawn(function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
    end)
end

function simpleremote()
    spawn(function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/exxtremestuffs/SimpleSpySource/master/SimpleSpy.lua"))()
    end)
end

function ut3dbypass()
    spawn(function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/emkodelirdi/EmkoDelirdi/main/ut3dbbbypass"))()
    end)
end

MiscTab:AddToggle({
	Name = "Auto Fight",
	Default = false,
	Callback = function(Value)
		getgenv().autofighthal = Value
        autoFight()
	end    
})

TPTab:AddToggle({
	Name = "Auto Grab Pumps",
	Default = false,
	Callback = function(Value)
		getgenv().autograbpumpkin = Value
        collectpumps()
	end    
})

TPTab:AddButton({
	Name = "TP to Asriel",
	Callback = function()
		tptoasriel()
  	end    
})

