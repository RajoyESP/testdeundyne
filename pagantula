local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Undertale 3D Boss Battles", IntroEnabled = true, IntroText = "Paquito's Productions", HidePremium = false, SaveConfig = true, ConfigFolder = "Facherumtest"})

--Values
getgenv().autofighthal = true;
getgenv().autograbpumpkin = true;
getgenv().autofood = true;

--Tab Creation
local AutoTab = Window:MakeTab({
	Name = "Morphs",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local TPTab = Window:MakeTab({
	Name = "Tps",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local MiscTab = Window:MakeTab({
	Name = "Miscellanious",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

--Morphs
function equipmorph()
        spawn(function()
        local args = {
            [1] = game:GetService("Players").LocalPlayer,
            [2] = "ReaperSansMorph" --Reaper Sans
        }
        game:GetService("ReplicatedStorage").MorphEquipRE:FireServer(unpack(args))
    end)
end

function equipmorph2()
    spawn(function()
    local args = {
        [1] = game:GetService("Players").LocalPlayer,
        [2] = "KillerSansMorph" --Killer Sans
    }
    game:GetService("ReplicatedStorage").MorphEquipRE:FireServer(unpack(args))
end)
end

